package com.Chord.data;

import com.Chord.tool.SHA1;

public class Node {
	private String ip;
	private String port;
	private long hashCode;
	public Node(String ip,String port) {
		this.ip = ip;
		this.port = port;
		System.out.println(ip+":"+port.toString());
		this.hashCode = SHA1.GetHash(ip+":"+port.toString());
	}
}
