package com.Net;

import java.net.DatagramPacket;
import java.util.LinkedList;
import java.util.Queue;

public class SeverDataProcessing implements Runnable{
	public Queue<DatagramPacket> DataQueue = new LinkedList<DatagramPacket>();
	
	public void run() {
		while(true) {
				if(!DataQueue.isEmpty()) {
					DatagramPacket dp = DataQueue.poll();
					System.out.println("收到包： ip:"+dp.getAddress().getHostAddress()+"	port:"+dp.getPort()+"		数据头： data:"+ new String(dp.getData()));
				}

		}
	}
}